macros {
	dquo_space: dquo_space {
		compatible = "zmk,behavior-macro";
		#binding-cells = <0>;
		bindings =
		<&macro_release>,
		<&kp LSHFT &kp RSHFT &kp LALT &kp RALT &kp LCTRL &kp RCTRL &kp LGUI &kp RGUI>,
		<&macro_tap>,
		<&kp DQT &kp SPACE>;
		label = "DQUO_SPACE";
	};
	
	squo_space: squo_space {
		compatible = "zmk,behavior-macro";
